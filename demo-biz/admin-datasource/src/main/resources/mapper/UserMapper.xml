<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--@author wlpiaoyi:WLPIAOYI-DELL-->
<!--@since 2024-10-11 18:08:10-->
<!--@desc 用户表-->
<mapper namespace="org.wlpiaoyi.server.demo.sys.domain.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="userResultMap" type="org.wlpiaoyi.server.demo.sys.domain.entity.User">
        <id column="id" property="id"/>
        <!-- 账号 -->
        <result column="account" property="account"/>
        <!-- 密码 -->
        <result column="password" property="password"/>
        <!-- 部门ID -->
        <result column="dept_id" property="deptId"/>
        <!-- 当前角色Id -->
        <result column="cur_role_id" property="curRoleId"/>
        <!-- 状态 -->
        <result column="status" property="status"/>
        <!-- 是否删除 -->
        <result column="is_deleted" property="isDeleted"/>
        <!-- 创建人 -->
        <result column="create_user" property="createUser"/>
        <!-- 创建时间 -->
        <result column="create_time" property="createTime"/>
        <!-- 修改人 -->
        <result column="update_user" property="updateUser"/>
        <!-- 修改时间 -->
        <result column="update_time" property="updateTime"/>
    </resultMap>

</mapper>
